- profile ||= nil
- album ||= nil
- profile_id = profile&.id || photo.album.profile_id
- album_id = album&.id || photo.album_id

.col-xs-6.col-sm-4.col-lg-3
  = link_to('', profile_album_photo_path(profile_id, album_id, photo), class: "photo-thumb",
    style: "background-image: url('#{photo.image}');")
